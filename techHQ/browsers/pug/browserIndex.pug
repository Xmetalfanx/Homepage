    h3 Browser Addons
    #total-addons
        #addons-container.d-flex.flex-dir-rows.jc-around
            .addon.w-fitcontent
                img.icon(src="/techHQ/browsers/graphics/icons/ublock-origin.svg" alt="Ublock Origin SVG Icon")
                .fw-800 Ublock Origin
                .fs-italics Ad/Content Blocker
                ul.d-flex
                    li
                        a(href="https://addons.mozilla.org/en-US/firefox/addon/ublock-origin/")
                            img.icon(src="/techHQ/progIcons/internet/browsers/firefox.svg" alt="Ublock Origin Firefox")
                    li
                        a(href="https://chrome.google.com/webstore/detail/ublock-origin/cjpalhdlnbpafiamejdnhcphjbkeiagm" target="_blank")
                            img.icon(src="/techHQ/progIcons/internet/browsers/chrome.svg" alt="Ublock Origin Chrome Store")
                    li
                        a(href="https://github.com/gorhill/uBlock" target="_blank")
                            img.icon(src="/graphics/simpleIcons/github.svg" alt="Ublock Origin Github")
            .addon
                img.icon(src="/techHQ/browsers/graphics/icons/bit-warden.svg" alt="Bit Warden SVG Icon")

                .fw-800 Bit Warden
                .fs-italics Open-Source Password Manager
                ul.d-flex
                    li
                        a(href="https://addons.mozilla.org/en-US/firefox/addon/bitwarden-password-manager/" target="_blank")
                            img.icon(src="/techHQ/progIcons/internet/browsers/firefox.svg" alt="Bit Warden Firefox")
                    li
                        a(href="https://chrome.google.com/webstore/detail/bitwarden-free-password-m/nngceckbapebfimnlniiiahkandclblb" target="_blank")
                            img.icon(src="/techHQ/progIcons/internet/browsers/chrome.svg" alt="Bit Warden Chrome Store")
                    li
                        a(href="https://github.com/bitwarden/clients" target="_blank")
                            img.icon(src="/graphics/simpleIcons/github.svg" alt="Bit Warden Github")

